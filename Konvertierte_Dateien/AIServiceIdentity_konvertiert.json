            <summary>
            Initializes a new instance of <see cref="T:Microsoft.Azure.CopilotForAzure.AIService.Identity.CopilotForAzureOnBehalfOfCredential"/>.
            </summary>
            <param name="appRegistrationOptions"></param>
            <param name="audienceRules"></param>
            <param name="environment"></param>
            <param name="talkToAzureResourcesCredential"></param>
            <param name="assignedManagedIdentities"></param>
            <param name="logger"></param>
        </member>
