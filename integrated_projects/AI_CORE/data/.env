# System Configuration
SYSTEM_MODE=production
LOG_LEVEL=INFO
DATABASE_PATH=wealth_system.db
LOG_FILE=wealth_system.log
DEBUG=false

# PayPal Business (Sandbox)
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=your_sandbox_client_id
PAYPAL_CLIENT_SECRET=your_sandbox_client_secret
PAYPAL_RECIPIENT_EMAIL=your-email@example.com

# Crypto Trading
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret

# AI Art Generation
OPENAI_API_KEY=your_openai_api_key
AI_ART_PROVIDER=dalle
MIDJOURNEY_API_KEY=your_midjourney_api_key

# Stripe (Alternative Payment)
STRIPE_API_KEY=your_stripe_api_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Error Monitoring
SENTRY_DSN=your_sentry_dsn

# Database Backup
BACKUP_ENABLED=true
BACKUP_INTERVAL=3600

# Compliance
BUSINESS_LICENSE=your_license_number
TAX_ID=your_tax_id
